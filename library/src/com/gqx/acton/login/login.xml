<struts>
	<package name="userLogin"   extends="struts-default">
		<action name="loginAction_*" class="com.gqx.acton.login.LoginAction"  method="{1}">
			 <result name="loginSuccess1" type="redirect">/index.jsp</result>
			 <result name="loginSuccess2" type="redirect">/index.jsp</result>
			 <result name="loginSuccess3" type="redirect">/index.jsp</result>
			 <result name="loginSuccess4" type="redirect">/index.jsp</result>
			 <result name="loginSuccess5" type="redirect">/index.jsp</result>
			 <result name="loginFail">/index.jsp</result>
			 <result name="loginSuccess" type="redirect">/index.jsp</result>
		</action>
	</package>
	
</struts>

