<!-- START SNIPPET: xworkSample -->
<struts>
	<package name="check"   extends="struts-default" namespace="/">
		<action name="check_*" class="com.gqx.acton.lendIDAdminister.LendIDAdminister"  method="{1}">
			<result name="oks" type="dispatcher">/lendIDAdminster.jsp</result>
		</action>
		
		<action name="handleRd_*"  class="com.gqx.acton.lendIDAdminister.HandleId"  method="{1}">
			<result name="AddSuccess"  type="dispatcher">/lendIDAdminster.jsp</result>
			<result name="AddFail">/index.jsp</result>
		</action>
	</package>
	
</struts>

<!-- END SNIPPET: xworkSample -->

